let arr = [2,4,-3,7];

function pair(arr,k) {
    for (let i = 0; i < arr.length; i++) {
        for (let j = i+1; j < arr.length; j++) {
            if (arr[i] + arr[j]==k && arr[i]!=arr[j]) {
              return true
        }
    }
    return false
 
}
}

const pairValues=pair(arr,10);

console.log(pairValues)
